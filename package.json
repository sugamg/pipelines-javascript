    "build:azureUATDE": "cross-env NODE_ENV=test TARGET_ENV=azureUATDE node --max_old_space_size=8192 scripts/build.js",
    "build:azureSTG": "cross-env NODE_ENV=production TARGET_ENV=azureSTG node --max_old_space_size=8192 scripts/build.js",
    "build:azureDEMO": "cross-env NODE_ENV=production TARGET_ENV=azureDEMO node --max_old_space_size=8192 scripts/build.js",
    "build:azureDEMOEU": "cross-env NODE_ENV=production TARGET_ENV=azureDEMOEU node --max_old_space_size=8192 scripts/build.js",
    "build:azurePROD": "cross-env NODE_ENV=production TARGET_ENV=azurePROD node --max_old_space_size=8192 scripts/build.js",
    "build:azurePRODDE": "cross-env NODE_ENV=production TARGET_ENV=azurePRODDE node --max_old_space_size=8192 scripts/build.js",
    "build": "node --max_old_space_size=8192 scripts/build.js",
    "test": "cross-env TARGET_ENV=local NODE_ENV=test jest",
    "test:coverage": "cross-env TARGET_ENV=local NODE_ENV=test jest --coverage",
    "test:watch": "jest --watch",
    "prettier": "find client/ -name \"*.js\" | xargs prettier --config config/prettier.config.js",
    "mock": "node scripts/mock/mockApi.js",
    "mock:data": "node scripts/mock/generateData.js",
    "start": "cross-env NODE_ENV=production node server/server.js",
    "refreshVSToken": "vsts-npm-auth -config .npmrc"
  },
