  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "eslintConfig": {
    "plugins": [
      "jsx-a11y"
    ],
    "extends": [
      "react-app",
      "plugin:jsx-a11y/recommended"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --config config/prettier.config.js --single-quote --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testURL": "http://localhost/",
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      ".test.js",
      ".spec.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,js}"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src/$1",
      "\\.(less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(css)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./config/jest-setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "**/tests/**/*.[jt]s?(x)",
      "**/?(*.)+(test).[jt]s?(x)"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(material-ui-next-pickers)/)"
    ]
  }
}
