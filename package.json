{
    "name": "nova",
    "version": "1.0.0-1001",
    "scripts": {
        "ng": "ng",
        "start": "node --max_old_space_size=8196 ./node_modules/@angular/cli/bin/ng serve --port=8080 --poll=1000",
        "start:nomaps": "node --max_old_space_size=8196 ./node_modules/@angular/cli/bin/ng serve --port=8080 --poll=1000 --sourceMap=false --open",
        "start:prod": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve --prod --port=8080",
        "build": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build",
        "build:stats": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --stats-json",
        "build:profile": "set NG_BUILD_PROFILING=1 && node --max_old_space_size=16000 ./node_modules/@angular/cli/bin/ng build --verbose",
        "build:prod": "node --max_old_space_size=8196 ./node_modules/@angular/cli/bin/ng build --sourceMap=false --prod",
        "build-prod": "node --max_old_space_size=8196 ./node_modules/@angular/cli/bin/ng build --sourceMap=false --prod",
        "build-prod:stats": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --sourceMap=false --prod --stats-json",
        "test": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng test --karmaConfig=./karma.conf.js --sourceMap=false",
        "test:ci": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng test --karmaConfig=./karma.conf.js --sourceMap=false --watch=false --browsers=ChromeHeadlessCI",
        "test:once": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng test --watch=false",
        "lint": "ng lint",
        "eslint": "eslint . --ext .ts",
        "eslint:fix": "eslint . --ext .ts --fix",
        "compodoc:serve": "compodoc src -p tsconfig.json -s --theme material",
        "compodoc:generate": "compodoc src -p tsconfig.json --theme material",
        "e2e": "ng e2e",
        "bundle-report": "webpack-bundle-analyzer dist/stats.json",
        "prod-stats": "npm run build-prod:stats && npm run bundle-report",
        "non-prod-stats": "npm run build:stats && npm run bundle-report",
        "postinstall": "ngcc && patch-package",
        "sonar-scanner": "sonnar-scanner",
        "refreshVSToken": "vsts-npm-auth -config .npmrc"
    },
    "private": true,
    "browserslist": [
        "last 1 Chrome version",
        "not IE 9-11"
